<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
     
        
<bean id="student" class="com.model.Student">
<constructor-arg value="Janu"></constructor-arg>
<constructor-arg value="22"></constructor-arg>
</bean>

<bean id="employee" class="com.model.Employee" parent="student">
<constructor-arg value="Chennai"></constructor-arg>
<constructor-arg value="10000"></constructor-arg>
</bean>
</beans>